              a.link.twitter(href='https://twitter.com/jamesjlee8', target='_parent')
                  span.fa.fa-twitter
              a.link.google-plus(href='https://plus.google.com/113502061889985148153', target='_parent')
                  span.fa.fa-google-plus-square
              a.link.github(href='https://github.com/jamesjlee', target='_parent')
                  span.fa.fa-github
              a.link.download(href='https://drive.google.com/file/d/0Byx3CHdqtZ8qMWl0Rm5ibHVzNnM/edit?usp=sharing', target='_parent')
